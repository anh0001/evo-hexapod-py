  <link name="base_link">
    <visual>
      <geometry>
        <box size="1.0 0.4 0.2"/>
      </geometry>
      <material name="green">
        <color rgba="0.3 0.7 0.3 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="1.0 0.4 0.2"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1.0"/>
      <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
